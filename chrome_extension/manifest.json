{

	"name": "Pigeon | Connexus",
	"version": "1.0",
	"manifest_version": 2,
	"description": "This is the Connexus extension for group Pigeon",
	"permissions": [
    "identity",
    "identity.email"
  ],
	"browser_action": {
		"default_icon": "pigeon.png",
		"default_popup": "upload.html"
	},
    "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://maps.googleapis.com; object-src 'self'"

}